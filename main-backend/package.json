{
  "name": "wellnoosh-main-backend",
  "version": "1.0.0",
  "description": "Backend API for WellNoosh nutrition app",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "migrate": "ts-node src/utils/migrate.ts",
    "check-email": "ts-node src/utils/disableEmailConfirmation.ts",
    "confirm-user": "ts-node src/utils/confirmUser.ts",
    "delete-user": "ts-node src/utils/deleteUser.ts",
    "create-profile": "ts-node src/utils/createUserProfile.ts",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "db:rollback": "knex migrate:rollback"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "openai": "^6.15.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.5",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.7",
    "@types/multer": "^1.4.10",
    "@types/node": "^20.9.0",
    "@types/nodemailer": "^6.4.13",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "nutrition",
    "recipes",
    "health",
    "meal-planning",
    "api"
  ],
  "author": "WellNoosh Team",
  "license": "MIT"
}
