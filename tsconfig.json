{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "nativewind-env.d.ts"
  ],
  "exclude": [
    "node_modules",
    "V3/**/*",
    "legacy/**/*",
    "screens/**/*",
    "services/**/*",
    "hooks/useGoogleAuth.ts"
  ]
}
